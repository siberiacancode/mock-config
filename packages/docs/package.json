    "lucide-react": "^0.544.0",
    "next": "16.0.10",
    "react": "^19.2.1",
    "react-dom": "^19.2.0",
    "twoslash": "^0.3.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/serve-handler": "^6.1.4",
    "postcss": "^8.5.6",
    "serve-handler": "^6.1.6",
    "tailwindcss": "^4.1.13"
